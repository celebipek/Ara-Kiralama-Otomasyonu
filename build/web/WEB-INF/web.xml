<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <error-page>
        <location>/error.jsp</location>
    </error-page>
    <servlet>
        <servlet-name>NewUserServlet</servlet-name>
        <servlet-class>User.NewUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewCarServlet</servlet-name>
        <servlet-class>Car.NewCarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditCarServlet</servlet-name>
        <servlet-class>Car.EditCarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteCarServlet</servlet-name>
        <servlet-class>Car.DeleteCarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginUserServlet</servlet-name>
        <servlet-class>User.LoginUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginAdminServlet</servlet-name>
        <servlet-class>Admin.LoginAdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RentCarServlet</servlet-name>
        <servlet-class>Rent_Info.RentCarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StopRentServlet</servlet-name>
        <servlet-class>Rent_Info.StopRentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteUserServlet</servlet-name>
        <servlet-class>User.DeleteUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewUserServlet</servlet-name>
        <url-pattern>/NewUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewCarServlet</servlet-name>
        <url-pattern>/NewCarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditCarServlet</servlet-name>
        <url-pattern>/EditCarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCarServlet</servlet-name>
        <url-pattern>/DeleteCarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginUserServlet</servlet-name>
        <url-pattern>/LoginUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginAdminServlet</servlet-name>
        <url-pattern>/LoginAdminServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RentCarServlet</servlet-name>
        <url-pattern>/RentCarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StopRentServlet</servlet-name>
        <url-pattern>/StopRentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteUserServlet</servlet-name>
        <url-pattern>/DeleteUserServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
